pipeline {
  agent any
  stages {
    stage('install') {
      steps {
        echo 'Good'
      }
    }

    stage('build') {
      steps {
        echo 'Hello World'
      }
    }

    stage('run') {
      steps {
        echo 'running'
      }
    }

    stage('test') {
      steps {
        echo 'test'
      }
    }

  }
}